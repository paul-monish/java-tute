package inheritance.prac;

class A{
	void showA() {
		System.out.println("In A");
	}
}
class B extends A{
	void showB() {
		System.out.println("In B");
	}
}
class C extends B{
	void showC() {
		System.out.println("In C");
	}
}


public class MultilevelInheritance {
	public static void main(String[] args) {

	}
}
